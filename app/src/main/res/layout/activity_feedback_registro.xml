<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="#ef8732"
    android:padding="32dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Feedback_Registro">
    
    <ImageView
        android:id="@+id/logoCheck"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/baseline_check_circle_outline_24"
        app:tint="@color/blue"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/textRegistroOk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="32dp"/>

    <TextView
        android:id="@+id/textRegistroOk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/tx_grande"
        android:textColor="@color/black"
        android:text="@string/Registro_exitoso"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/logoCheck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="34dp" />





</androidx.constraintlayout.widget.ConstraintLayout>